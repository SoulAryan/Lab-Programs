#include <stdio.h>
int main()
{
    int num1,num2,num3;

    printf("Enter num1=");
    scanf("%d",&num1);

    printf("Enter num2=");
    scanf("%d",&num2);

    printf("Enter num3=");
    scanf("%d",&num3);

    if(num1>num2)
    {
        printf("num1 %d is greater than num2 %d",num1,num2);
    }

    else if(num1>num3)
    {
        printf("num1 %d is greater than num3 %d",num1,num3);
    }

    else if(num2>num1)
    {
       printf("num2 %d is greater than num1 %d",num2,num1);
    }

    else if(num2>num3)
    {
       printf("num2 %d is greater than num3 %d",num2,num3);
    }

     else if(num3>num1)
     {
       printf("num3 %d is greater than num1 %d",num3,num1);
     }

     else if(num3>num2)
     {
       printf("num3 %d is greater than num2 %d",num3,num2);
     }

    return 0;
}